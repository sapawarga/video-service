{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
        "*"
      ],
      "expose_headers": [
        "Content-Length",
        "application/json"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "DELETE",
        "CONNECT",
        "OPTIONS",
        "TRACE",
        "PATCH"
      ],
      "allow_credentials": false
    },
    "github_com/devopsfaith/krakend-botdetector": {
      "cacheSize": 0
    }
  },
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "name": "sapawarga-toggler-app",
  "disable_rest": true,
  "endpoints": [
    {
      "endpoint": "/videos/",
      "method": "GET",
      "querystring_params": [
        "regency_id",
        "page"
      ],
      "backend": [
        {
          "url_pattern": "/videos/",
          "host": [
            "0.0.0.0:3001"
          ]
        }
      ],
      "headers_to_pass": [
        "token"
      ]
    }, {
      "endpoint": "/videos/",
      "method": "POST",
      "backend": [
        {
          "url_pattern": "/videos/",
          "host": [
            "0.0.0.0:3001"
          ]
        }
      ],
      "headers_to_pass": [
        "token"
      ]
    }, {
      "endpoint": "/videos/{id}",
      "method": "GET",
      "backend": [
        {
          "url_pattern": "/videos/{id}",
          "host": [
            "0.0.0.0:3001"
          ]
        }
      ],
      "headers_to_pass": [
        "token"
      ]
    }, {
      "endpoint": "/videos/{id}",
      "method": "PUT",
      "backend": [
        {
          "url_pattern": "/videos/{id}",
          "host": [
            "0.0.0.0:3001"
          ]
        }
      ],
      "headers_to_pass": [
        "token"
      ]
    }, {
      "endpoint": "/videos/{id}",
      "method": "DELETE",
      "backend": [
        {
          "url_pattern": "/videos/{id}",
          "host": [
            "0.0.0.0:3001"
          ]
        }
      ],
      "headers_to_pass": [
        "token"
      ]
    }
  ]
}